{
	"name": "ziwot/cake-tezos",
	"description": "Tezos plugin for CakePHP",
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "steven j",
			"email": "hi@sjehan.net"
		}
	],
	"require": {
		"php": ">=8.1",
		"cakephp/cakephp": "^5.0.1",
		"ziwot/pezos": "^0.2.0",
		"cakephp/authentication": "^3.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.1"
	},
	"autoload": {
		"psr-4": {
			"CakeTezos\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CakeTezos\\Test\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
